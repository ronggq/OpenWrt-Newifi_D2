CONFIG_TARGET_ramips=y
CONFIG_TARGET_ramips_mt7621=y
CONFIG_TARGET_ramips_mt7621_DEVICE_d-team_newifi-d2=y

# CONFIG_SIGNATURE_CHECK is not set
# CONFIG_SIGNED_PACKAGES is not set

# IPv6 支持
CONFIG_IPV6=y
CONFIG_KERNEL_IPV6=y
CONFIG_PACKAGE_ipv6helper=y
CONFIG_PACKAGE_dnsmasq_full_dhcpv6=y
CONFIG_PACKAGE_6in4=y
CONFIG_PACKAGE_6rd=y
CONFIG_PACKAGE_6to4=y

# Wireless 驱动程序(开源，与闭源任意选一个)
# CONFIG_PACKAGE_kmod-mt76 is not set

# Wireless 驱动程序(闭源)
CONFIG_PACKAGE_luci-app-mtwifi=y
CONFIG_PACKAGE_mt_wifi=y
CONFIG_PACKAGE_kmod-mt7603e=y
CONFIG_PACKAGE_kmod-mt76x2e=y

# USB 相关
CONFIG_PACKAGE_automount=y
CONFIG_PACKAGE_e2fsprogs=y
CONFIG_PACKAGE_smartmontools=y
CONFIG_PACKAGE_luci-app-hd-idle=y                # 硬盘休眠

# usb打印机
CONFIG_PACKAGE_luci-app-usb-printer=y
CONFIG_PACKAGE_kmod-usb-printer=y
CONFIG_PACKAGE_luci-i18n-usb-printer-zh-cn=y
CONFIG_PACKAGE_p910nd=y

# 关闭USB3
CONFIG_PACKAGE_luci-app-usb3disable=y
CONFIG_PACKAGE_luci-i18n-usb3disable-zh-cn=y

## USB 网络共享
CONFIG_PACKAGE_kmod-usb-net=y
CONFIG_PACKAGE_kmod-usb-net-asix=y
CONFIG_PACKAGE_kmod-usb-net-cdc-ether=y
CONFIG_PACKAGE_kmod-usb-net-cdc-ncm=y
CONFIG_PACKAGE_kmod-usb-net-hso=y
CONFIG_PACKAGE_kmod-usb-net-huawei-cdc-ncm=y
CONFIG_PACKAGE_kmod-usb-net-ipheth=y
CONFIG_PACKAGE_kmod-usb-net-rndis=y
CONFIG_PACKAGE_usbutils=y

## USB 网络存储
CONFIG_PACKAGE_autosamba=y
CONFIG_PACKAGE_luci-app-samba=y                  # 网络共享（samba）
CONFIG_PACKAGE_luci-app-vsftpd=y                   # FTP 服务器

# LUCI Appliciations
CONFIG_PACKAGE_luci-app-autoreboot=y             # 计划重启
CONFIG_PACKAGE_luci-app-adbyby-plus=y          # 广告屏蔽大师
CONFIG_PACKAGE_luci-app-filetransfer=y                 # 文件传输（可web安装ipk包）
CONFIG_PACKAGE_luci-app-mwan3=y                    # MWAN 负载均衡
CONFIG_PACKAGE_luci-app-mwan3helper=y
CONFIG_PACKAGE_luci-app-upnp=y                # 通用即插即用 UPnP(端口自动转发)
CONFIG_PACKAGE_luci-app-unblockmusic=y    # 解锁网易云灰色歌曲3合1新版本
CONFIG_PACKAGE_luci-app-webadmin=y          # Web 管理页面设置
CONFIG_PACKAGE_luci-app-wol=y                     # 网络唤醒
CONFIG_PACKAGE_luci-app-ttyd=y                     # 网页终端命令行
CONFIG_PACKAGE_luci-app-turboacc=y        #Turbo ACC 网络加速(支持 Fast Path 或者 硬件 NAT)
CONFIG_PACKAGE_luci-app-vlmcsd=y                   # KMS 服务器（WIN 激活工具）
CONFIG_PACKAGE_luci-app-xlnetacc=y                 # 迅雷快鸟
CONFIG_PACKAGE_luci-app-watchcat=y               #断网检测功能与定时重启
CONFIG_PACKAGE_luci-app-zerotier=y               # ZeroTier内网穿透
